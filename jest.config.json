{
  "preset": "ts-jest",
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/main/interface.ts"
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/main/**/*.ts"
  ],
  "testMatch": [
    "<rootDir>/src/test/*.ts"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/src/test/stub"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ]
}
